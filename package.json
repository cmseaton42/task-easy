{
    "name": "micro-queue",
    "version": "0.1.0",
    "description": "A lightwieght promise-based priority task/job queue.",
    "main": "./micro-queue/index.js",
    "scripts": {
        "test":
            "npm run lint && jest && npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "test:local": "npm run lint && jest && npm run test:coverage",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:detailed": "jest --verbose",
        "test:update": "jest -u",
        "lint": "./node_modules/.bin/eslint . --ext .js",
        "lint:fix": "npm run lint -- --fix"
    },
    "keywords": [
      "queue",
      "task",
      "worker",
      "job",
      "in-memory",
      "priority",
      "manager",
      "utility"
    ],
    "author": "Canaan Seaton",
    "license": "MIT",
    "devDependencies": {
        "coveralls": "^3.0.0",
        "eslint": "^4.19.1",
        "eslint-plugin-jest": "^21.15.0",
        "jest": "^22.4.3"
    }
}
